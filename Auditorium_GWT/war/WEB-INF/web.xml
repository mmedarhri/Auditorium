<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

	<!-- Servlets -->
	<servlet>
		<servlet-name>calendar</servlet-name>
		<servlet-class>com.example.calendar.server.CalendarServiceImpl</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>calendar</servlet-name>
		<url-pattern>/calendar/greet</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>com.ensisa.test.login.server.MyServiceImpl</servlet-class>
	</servlet>


<servlet-mapping>
  <servlet-name>login</servlet-name>
  <url-pattern>/login/MyService</url-pattern>
</servlet-mapping>

<servlet-mapping>
  <servlet-name>login</servlet-name>
  <url-pattern>/calendar/MyService</url-pattern>
</servlet-mapping>
	<servlet>
		<servlet-name>table</servlet-name>
		<servlet-class>com.ensisa.table.server.AuditionCalendarServiceImpl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>table</servlet-name>
		<url-pattern>/dynatable/table</url-pattern>
	</servlet-mapping>

	<!-- Default page to serve -->
	<welcome-file-list>
		<welcome-file>Calendar.html</welcome-file>
		<welcome-file>Login.html</welcome-file>
		<welcome-file>DynaTable.html</welcome-file>
				<welcome-file>Jury.html</welcome-file>
					<welcome-file>Statistiques.html</welcome-file>
	</welcome-file-list>

</web-app>
